title: search-data.xml
---
<?xml version="1.0" encoding="UTF-8" ?>
<articles>
    {% for post in site.posts %}
    <article>
        <title>{{ post.title }}</title>
        <url>{{ site.baseurl }}{{ post.url }}</url>
        <date>{{ post.date | date: '%Y-%m-%d'}}</date>
    </article>
    {% endfor %}
</articles>